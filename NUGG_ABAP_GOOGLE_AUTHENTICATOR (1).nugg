   <parameter CLSNAME="ZGOOGLE_AUTHENTICATE" CMPNAME="BASE32_DECODE" SCONAME="BASE32_STRING" VERSION="1" LANGU="E" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " AUTHOR="CHRISP" CREATEDON="20110712" CHANGEDON="00000000" PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="STRING"/>

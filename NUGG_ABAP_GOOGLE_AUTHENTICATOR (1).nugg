   <source>method base32_decode.

*  implements base 32 as defined by rfc 4648
* http://tools.ietf.org/html/rfc4648

* we need to encode  SECRET_BITS/8 bytes of data

* ABAP can be so nice sometimes!

  data: l_encoded_len type i,
        l_five_bit_value type i,
        l_bit_position type i,
        l_encoded_char type i,
        l_bit_pos type i,
        l_char type i,
        l_bit type i,
        l_power type i,
        l_xstring type xstring,
        l_encoded type c.

  l_encoded_len = strlen( base32_string ).

  l_xstring = &apos;00000000000000000000&apos;.



  do l_encoded_len times.

    l_encoded = base32_string+l_encoded_char(1).

    find first occurrence of l_encoded in c_rfc4648_base32 match offset l_five_bit_value.

    l_power = 4.

    do 5 times.
      l_char = l_bit_position div 8.
      l_bit_pos =  l_bit_position + 1.

      l_bit = ( l_five_bit_value div ( 2 ** l_power ) ) mod 2.
      set bit l_bit_pos of l_xstring to l_bit.

      l_power = l_power - 1.
      l_bit_position = l_bit_position + 1.
    enddo.
    l_encoded_char = l_encoded_char + 1.
  enddo.

  xstring_value = l_xstring.
endmethod.</source>
  </method>
  <method CLSNAME="ZGOOGLE_AUTHENTICATE" CMPNAME="BASE32_ENCODE" VERSION="1" LANGU="E" EXPOSURE="2" STATE="1" EDITORDER="10 " DISPID="0 " AUTHOR="CHRISP" CREATEDON="20110707" CHANGEDBY="CHRISP" CHANGEDON="20110725" MTDTYPE="0" MTDDECLTYP="1" R3RELEASE="701" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZGOOGLE_AUTHENTICATE" CMPNAME="BASE32_ENCODE" SCONAME="RANDOM_XSTRING" VERSION="1" LANGU="E" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " AUTHOR="CHRISP" CREATEDON="20110707" CHANGEDON="00000000" PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="XSTRING"/>
   <parameter CLSNAME="ZGOOGLE_AUTHENTICATE" CMPNAME="BASE32_ENCODE" SCONAME="ENCODED" VERSION="1" LANGU="E" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " AUTHOR="CHRISP" CREATEDON="20110707" CHANGEDON="00000000" PARDECLTYP="3" PARPASSTYP="0" TYPTYPE="1" TYPE="STRING"/>
   <source>method base32_encode.

* implements base 32 as defined by RFC 4648
* http://tools.ietf.org/html/rfc4648

* we need to encode  SECRET_BITS/8 bytes of data

* ABAP can be so nice sometimes!

  data: l_encoded_len type i,
        l_five_bit_value type i,
        l_bit_position type i,
        l_bit_pos type i,
        l_char type i,
        l_bit type i,
        l_power type i,
        l_encoded type c.

  l_encoded_len = xstrlen( random_xstring ) * 4 / 5.

  do l_encoded_len times.
    l_power = 4.
    l_five_bit_value = 0.
    do 5 times.
      l_char = l_bit_position div 8.
      l_bit_pos = ( l_bit_position mod 8 ) + 1.
      get bit l_bit_pos of random_xstring+l_char(1) into l_bit.
      l_five_bit_value = l_five_bit_value + ( ( 2 ** l_power ) * l_bit ).
      l_power = l_power - 1.
      l_bit_position = l_bit_position + 1.
    enddo.

* convert this into 32 character encoding

    l_encoded = c_rfc4648_base32+l_five_bit_value(1).

    concatenate encoded l_encoded into encoded.

  enddo.

endmethod.</source>
  </method>
  <method CLSNAME="ZGOOGLE_AUTHENTICATE" CMPNAME="CHECK_CODE" VERSION="1" LANGU="E" DESCRIPT="Checks if a code matches a given secret" EXPOSURE="2" STATE="1" EDITORDER="7 " DISPID="0 " AUTHOR="CHRISP" CREATEDON="20110709" CHANGEDBY="CHRISP" CHANGEDON="20110725" MTDTYPE="0" MTDDECLTYP="0" MTDNEWEXC="X" R3RELEASE="701" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZGOOGLE_AUTHENTICATE" CMPNAME="CHECK_CODE" SCONAME="CODE" VERSION="1" LANGU="E" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " AUTHOR="CHRISP" CREATEDON="20110709" CHANGEDON="00000000" PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="STRING"/>
   <parameter CLSNAME="ZGOOGLE_AUTHENTICATE" CMPNAME="CHECK_CODE" SCONAME="IS_MATCH" VERSION="1" LANGU="E" DESCRIPT="Data element for domain BOOLE: TRUE (=&apos;X&apos;) and FALSE (=&apos; &apos;)" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " AUTHOR="CHRISP" CREATEDON="20110709" CHANGEDBY="CHRISP" CHANGEDON="20110709" PARDECLTYP="3" PARPASSTYP="0" TYPTYPE="1" TYPE="BOOLE_D"/>
   <exception CLSNAME="ZGOOGLE_AUTHENTICATE" CMPNAME="CHECK_CODE" SCONAME="ZCX_GOOGLE_SECRET_STORE" VERSION="1" LANGU="E" DESCRIPT="Exception for Google Secret store" MTDTYPE="0" EDITORDER="1 " AUTHOR="CHRISP" CREATEDON="20110725" CHANGEDON="00000000"/>
   <source>method check_code.

  data: l_secret_update_required type boole_d.

  if mo_secret_store is initial.
    raise exception type zcx_google_secret_store.
  endif.

* first of all check if this is one of the scratch codes
  check_scratch_codes(
    exporting
      code                   = code
    importing
      is_match               = is_match
      secret_update_required = l_secret_update_required ).

  if is_match = abap_false.

    check_timebased_code(
    exporting
      code                   = code
    importing
      is_match               = is_match
      secret_update_required = l_secret_update_required ).

  endif.

  if l_secret_update_required = abap_true.
    mo_secret_store-&gt;update_store( ).
  endif.
endmethod.</source>
  </method>
  <method CLSNAME="ZGOOGLE_AUTHENTICATE" CMPNAME="CHECK_SCRATCH_CODES" VERSION="1" LANGU="E" DESCRIPT="Check if code is a scratch code" EXPOSURE="0" STATE="1" EDITORDER="1 " DISPID="0 " AUTHOR="CHRISP" CREATEDON="20110709" CHANGEDBY="CHRISP" CHANGEDON="20110725" MTDTYPE="0" MTDDECLTYP="0" R3RELEASE="701" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZGOOGLE_AUTHENTICATE" CMPNAME="CHECK_SCRATCH_CODES" SCONAME="CODE" VERSION="1" LANGU="E" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " AUTHOR="CHRISP" CREATEDON="20110709" CHANGEDON="00000000" PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="STRING"/>
   <parameter CLSNAME="ZGOOGLE_AUTHENTICATE" CMPNAME="CHECK_SCRATCH_CODES" SCONAME="IS_MATCH" VERSION="1" LANGU="E" DESCRIPT="Data element for domain BOOLE: TRUE (=&apos;X&apos;) and FALSE (=&apos; &apos;)" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " AUTHOR="CHRISP" CREATEDON="20110709" CHANGEDON="00000000" PARDECLTYP="1" PARPASSTYP="1" TYPTYPE="1" TYPE="BOOLE_D"/>
   <parameter CLSNAME="ZGOOGLE_AUTHENTICATE" CMPNAME="CHECK_SCRATCH_CODES" SCONAME="SECRET_UPDATE_REQUIRED" VERSION="1" LANGU="E" DESCRIPT="Data element for domain BOOLE: TRUE (=&apos;X&apos;) and FALSE (=&apos; &apos;)" CMPTYPE="1" MTDTYPE="0" EDITORDER="3 " DISPID="0 " AUTHOR="CHRISP" CREATEDON="20110709" CHANGEDON="00000000" PARDECLTYP="1" PARPASSTYP="1" TYPTYPE="1" TYPE="BOOLE_D"/>
   <source>method check_scratch_codes.

* unfortunately just too simple

  data: lt_scratchcodes type stringtab,
        l_code type string,
        l_scratchcode type string.

  is_match = abap_false.
  secret_update_required = abap_false.

* make sure we have 8 digits and no spaces.
  l_code = code.
  condense l_code no-gaps.

  if l_code co &apos;0123456789&apos;.

* get scratchcodes
    lt_scratchcodes =  mo_secret_store-&gt;get_scratch_codes( ).
    loop at lt_scratchcodes into l_scratchcode.
      if l_scratchcode = l_code.
        is_match = abap_true.
        delete lt_scratchcodes.
        secret_update_required = abap_true.
      endif.
    endloop.

  endif.
endmethod.</source>
  </method>
  <method CLSNAME="ZGOOGLE_AUTHENTICATE" CMPNAME="CHECK_TIMEBASED_CODE" VERSION="1" LANGU="E" DESCRIPT="Check if code is a scratch code" EXPOSURE="0" STATE="1" EDITORDER="2 " DISPID="0 " AUTHOR="CHRISP" CREATEDON="20110709" CHANGEDBY="CHRISP" CHANGEDON="20110725" MTDTYPE="0" MTDDECLTYP="0" R3RELEASE="701" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZGOOGLE_AUTHENTICATE" CMPNAME="CHECK_TIMEBASED_CODE" SCONAME="CODE" VERSION="1" LANGU="E" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " AUTHOR="CHRISP" CREATEDON="20110709" CHANGEDON="00000000" PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="STRING"/>
   <parameter CLSNAME="ZGOOGLE_AUTHENTICATE" CMPNAME="CHECK_TIMEBASED_CODE" SCONAME="IS_MATCH" VERSION="1" LANGU="E" DESCRIPT="Data element for domain BOOLE: TRUE (=&apos;X&apos;) and FALSE (=&apos; &apos;)" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " AUTHOR="CHRISP" CREATEDON="20110709" CHANGEDON="00000000" PARDECLTYP="1" PARPASSTYP="1" TYPTYPE="1" TYPE="BOOLE_D"/>
   <parameter CLSNAME="ZGOOGLE_AUTHENTICATE" CMPNAME="CHECK_TIMEBASED_CODE" SCONAME="SECRET_UPDATE_REQUIRED" VERSION="1" LANGU="E" DESCRIPT="Data element for domain BOOLE: TRUE (=&apos;X&apos;) and FALSE (=&apos; &apos;)" CMPTYPE="1" MTDTYPE="0" EDITORDER="3 " DISPID="0 " AUTHOR="CHRISP" CREATEDON="20110709" CHANGEDON="00000000" PARDECLTYP="1" PARPASSTYP="1" TYPTYPE="1" TYPE="BOOLE_D"/>
   <source>method check_timebased_code.

  data: l_code type i,
        l_skew type i,
        l_timestamp type i,
        l_matching_code type i,
        l_window_size type i,
        l_max_time_skew type i,
        l_calculated_code type i,
        l_window_to_check type i,
        l_neg_window_to_check type i.

* must be a 6 digit code
  l_code = code.

  if l_code &lt;= 0 or l_code &gt; 999999.
    is_match = abap_false.
  else.

    is_match = abap_false.
* get previously saved skew
    l_skew = mo_secret_store-&gt;get_time_skew( ).

    l_window_size = mo_secret_store-&gt;get_window_size( ).

    l_timestamp = get_timestamp( ).

    l_window_to_check = l_timestamp - ( ( l_window_size - 1 ) / 2 ) + l_skew.

    do l_window_size times.

      l_calculated_code = compute_code( l_window_to_check ).
      if l_calculated_code = l_code.
        is_match = abap_true.
* stop this code being used again (if store supports that)
        secret_update_required = mo_secret_store-&gt;invalidate_timebased_code( l_window_to_check ).
      endif.
      l_window_to_check = l_window_to_check + 1.
    enddo.

    if is_match = abap_false.
* let&apos;s see if there is a match in the allowed time skew

      l_max_time_skew = mo_secret_store-&gt;get_max_skew_allowed( ).

      l_window_to_check = l_timestamp.
      l_neg_window_to_check = l_timestamp.
      do l_max_time_skew times.
        l_window_to_check = l_window_to_check + 1.
        l_neg_window_to_check = l_neg_window_to_check - 1.

        l_calculated_code = compute_code( l_window_to_check ).
        if l_calculated_code = l_code.
          is_match = abap_true.
          l_matching_code = l_window_to_check.
        endif.

        l_calculated_code = compute_code( l_neg_window_to_check ).
        if l_calculated_code = l_code.
          is_match = abap_true.
          l_matching_code = l_neg_window_to_check.
        endif.

        if is_match = abap_true.
* stop this code being used again (if store supports that)
          mo_secret_store-&gt;invalidate_timebased_code( l_code ).
* store new skew
          l_skew = l_matching_code - l_timestamp.
          is_match = mo_secret_store-&gt;set_time_skew( l_skew ).
          secret_update_required = abap_true.
          exit.
        endif.

      enddo.
    endif.

  endif.
endmethod.</source>
  </method>
  <method CLSNAME="ZGOOGLE_AUTHENTICATE" CMPNAME="COMPUTE_CODE" VERSION="1" LANGU="E" EXPOSURE="0" STATE="1" EDITORDER="3 " DISPID="0 " AUTHOR="CHRISP" CREATEDON="20110710" CHANGEDBY="CHRISP" CHANGEDON="20110725" MTDTYPE="0" MTDDECLTYP="0" R3RELEASE="701" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZGOOGLE_AUTHENTICATE" CMPNAME="COMPUTE_CODE" SCONAME="WINDOW_TO_CHECK" VERSION="1" LANGU="E" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " AUTHOR="CHRISP" CREATEDON="20110710" CHANGEDON="00000000" PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="I"/>
   <parameter CLSNAME="ZGOOGLE_AUTHENTICATE" CMPNAME="COMPUTE_CODE" SCONAME="CALCULATED_CODE" VERSION="1" LANGU="E" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " AUTHOR="CHRISP" CREATEDON="20110710" CHANGEDON="00000000" PARDECLTYP="3" PARPASSTYP="0" TYPTYPE="1" TYPE="I"/>
   <source>method compute_code.

  data: lo_hmac type ref to cl_abap_hmac,
        l_secret type string,
        l_key type xstring,
        l_data type xstring,
        l_ffffff7f type xstring,
        l_sha type xstring,
        l_padding type xstring,
        l_15 type x,
        l_bit type i,
        l_bit_position type i,
        l_power type i,
        l_data_length type i,
        l_offset type xstring,
        l_offset_int type i,
        lo_convert type ref to cl_abap_conv_out_ce,
        l_four_bytes type xstring.


* need to convert the secret into XSTRING with each character corresponding to a ASCII

  l_secret = mo_secret_store-&gt;get_secret( ).

* need to decode secret from base32 into xstring

  l_key = base32_decode( l_secret ).


  try.
      lo_hmac = cl_abap_hmac=&gt;get_instance( if_key = l_key ).

* get data in 8 byte format
* and just pad most significate bits with zeros.

      l_data = &apos;0000000000000000&apos;.

      l_power = 30.
      l_bit_position = 33.
      do 31 times.
        l_bit = ( window_to_check div ( 2 ** l_power ) ) mod 2.
        l_bit_position = l_bit_position + 1.
        set bit l_bit_position of l_data to l_bit.
        l_power = l_power - 1.
      enddo.

      l_data_length = xstrlen( l_data ).

      lo_hmac-&gt;update( if_data = l_data if_length = l_data_length ).

      lo_hmac-&gt;final( importing ef_hmacxstring = l_sha ).

    catch cx_abap_message_digest.
  endtry.

* right - now convert  4 bytes of this to an int4
* offset of which 4 bytes is given by last byte of result (position 39, 40 ) boolean
* AND with 0xF - so 0 to 15, so just the value of half-byte at position 39 (little-endian)

  l_offset = l_sha+19(1).
  l_15 = &apos;0F&apos;.
  l_offset = l_offset bit-and l_15.
  l_offset_int = l_offset.

  l_four_bytes = l_sha+l_offset_int(4).

* ensure it&apos;s not negative (first byte is most significant)
  l_ffffff7f = &apos;7FFFFFFF&apos;.

  l_four_bytes = l_four_bytes bit-and l_ffffff7f.

* convert to int.

  calculated_code = l_four_bytes.
* make 6 digits

  calculated_code = calculated_code mod 1000000.




endmethod.</source>
  </method>

  <interfaceMethod CLSNAME="ZGOOGLE_AUTHENTICATE_SECRET" CPDNAME="ZIF_GOOGLE_SECRET_STORE~GET_SCRATCH_CODES">
   <source>method zif_google_secret_store~get_scratch_codes.

  data: l_scratchcode type zgoogle_scratch_code.

  if mt_scratchcodes is initial.

* read scratch codes for current user

    do 6 times varying l_scratchcode from ms_secret-scratch_code1 next ms_secret-scratch_code2.

      if l_scratchcode is not initial.
        append l_scratchcode to mt_scratchcodes.
      endif.

    enddo.

  endif.

  scratch_codes = mt_scratchcodes.

endmethod.</source>
  </interfaceMethod>

  <method CLSNAME="ZGOOGLE_AUTHENTICATE_SECRET" CMPNAME="CONSTRUCTOR" VERSION="1" LANGU="E" DESCRIPT="CONSTRUCTOR" EXPOSURE="2" STATE="1" EDITORDER="2 " DISPID="0 " AUTHOR="CHRISP" CREATEDON="20110720" CHANGEDBY="CHRISP" CHANGEDON="20110720" MTDTYPE="2" MTDDECLTYP="0" MTDNEWEXC="X" R3RELEASE="701" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZGOOGLE_AUTHENTICATE_SECRET" CMPNAME="CONSTRUCTOR" SCONAME="USERNAME" VERSION="1" LANGU="E" DESCRIPT="User Name" CMPTYPE="1" MTDTYPE="2" EDITORDER="1 " DISPID="0 " AUTHOR="CHRISP" CREATEDON="20110720" CHANGEDON="00000000" PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="SYUNAME" PARVALUE="SY-UNAME"/>
   <exception CLSNAME="ZGOOGLE_AUTHENTICATE_SECRET" CMPNAME="CONSTRUCTOR" SCONAME="ZCX_GOOGLE_SECRET_STORE" VERSION="1" LANGU="E" DESCRIPT="Exception for Google Secret store" MTDTYPE="2" EDITORDER="1 " AUTHOR="CHRISP" CREATEDON="20110720" CHANGEDON="00000000"/>
   <source>method constructor.

* first off try to enqueue the store db - we&apos;ll hold the lock on this for the lifetime
* of this class.

    ms_secret-username = username.

  enqueue( ).

* ok - so we now have lock

  select single * from zgoogle_totp_key into ms_secret
    where username = username.

  if sy-subrc &lt;&gt; 0.
* no secret found!
* to allow easier processing set flag that no secret exists.
    m_no_secret = abap_true.
    ms_secret-username = username.
  endif.


endmethod.</source>
  </method>
  <method CLSNAME="ZGOOGLE_AUTHENTICATE_SECRET" CMPNAME="DEQUEUE" VERSION="1" LANGU="E" DESCRIPT="removes enqueue from user - class needs to be recreated" EXPOSURE="2" STATE="1" EDITORDER="0 " DISPID="0 " AUTHOR="CHRISP" CREATEDON="20110720" CHANGEDON="00000000" MTDTYPE="0" MTDDECLTYP="0" R3RELEASE="701" BCMTDCAT="00" BCMTDSYN="0">
   <source>method dequeue.

* remove enqueue

  call function &apos;DEQUEUE_EZ_GOOGLE_SECRET&apos;
    exporting
      username = ms_secret-username.

* set no-use flag

  m_enqueue = abap_false.

endmethod.</source>
  </method>
  <method CLSNAME="ZGOOGLE_AUTHENTICATE_SECRET" CMPNAME="ENQUEUE" VERSION="1" LANGU="E" EXPOSURE="2" STATE="1" EDITORDER="0 " DISPID="0 " AUTHOR="CHRISP" CREATEDON="20110720" CHANGEDBY="CHRISP" CHANGEDON="20110720" MTDTYPE="0" MTDDECLTYP="0" MTDNEWEXC="X" R3RELEASE="701" BCMTDCAT="00" BCMTDSYN="0">
   <exception CLSNAME="ZGOOGLE_AUTHENTICATE_SECRET" CMPNAME="ENQUEUE" SCONAME="ZCX_GOOGLE_SECRET_STORE" VERSION="1" LANGU="E" DESCRIPT="Exception for Google Secret store" MTDTYPE="0" EDITORDER="1 " AUTHOR="CHRISP" CREATEDON="20110720" CHANGEDON="00000000"/>
   <source>method enqueue.

  call function &apos;ENQUEUE_EZ_GOOGLE_SECRET&apos;
    exporting
      username       = ms_secret-username
    exceptions
      foreign_lock   = 1
      system_failure = 2
      others         = 3.
  if sy-subrc &lt;&gt; 0.
    raise exception type zcx_google_secret_store.
  endif.

* set enqueue flag - allows other methods to work

m_enqueue = abap_true.

endmethod.</source>
  </method>

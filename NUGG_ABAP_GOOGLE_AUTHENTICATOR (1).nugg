  data: l_view type string,
        l_create_code type boole_d,
        l_before_you_start type boole_d,
         l_show_scratch_code type boole_d.

  loop at it_interface_views into l_view.

    case l_view.
      when &apos;CREATE_CODE_W&apos;.
        l_create_code = abap_true.
      when &apos;BEFORE_YOU_START_W&apos;.
        l_before_you_start = abap_true.
      when &apos;SHOW_SCRATCH_CODES_W&apos;.
        l_show_scratch_code = abap_true.
    endcase.


  endloop.

  if l_before_you_start = abap_true.
    ev_result = process_event_before_you_start( io_event ).
  endif.
  if l_create_code = abap_true.
    ev_result = process_event_create_code( io_event ).
  endif.
  if l_show_scratch_code = abap_true.
    ev_result = process_event_show_scratch_cod( io_event ).
  endif.
endmethod.

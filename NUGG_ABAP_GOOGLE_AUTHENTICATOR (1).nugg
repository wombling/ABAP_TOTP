  <method CLSNAME="ZGOOGLE_AUTHENTICATE" CMPNAME="CREATE_SECRET" VERSION="1" LANGU="E" EXPOSURE="2" STATE="1" EDITORDER="11 " DISPID="0 " AUTHOR="CHRISP" CREATEDON="20110706" CHANGEDBY="CHRISP" CHANGEDON="20110725" MTDTYPE="0" MTDDECLTYP="0" R3RELEASE="701" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZGOOGLE_AUTHENTICATE" CMPNAME="CREATE_SECRET" SCONAME="SECRET_STORE" VERSION="1" LANGU="E" DESCRIPT="Store and access secret for user" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " AUTHOR="CHRISP" CREATEDON="20110709" CHANGEDBY="CHRISP" CHANGEDON="20110722" PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="3" TYPE="ZIF_GOOGLE_SECRET_STORE"/>
   <parameter CLSNAME="ZGOOGLE_AUTHENTICATE" CMPNAME="CREATE_SECRET" SCONAME="MULTI_USE_ALLOWED" VERSION="1" LANGU="E" DESCRIPT="Using the same token multiple times is allowed" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " AUTHOR="CHRISP" CREATEDON="20110706" CHANGEDBY="CHRISP" CHANGEDON="20110722" PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="BOOLE_D" PARVALUE="ABAP_FALSE" PARPREFERD="X"/>
   <parameter CLSNAME="ZGOOGLE_AUTHENTICATE" CMPNAME="CREATE_SECRET" SCONAME="TIME_WINDOW" VERSION="1" LANGU="E" DESCRIPT="Number of 30 blocks either side of current time allowed" CMPTYPE="1" MTDTYPE="0" EDITORDER="3 " DISPID="0 " AUTHOR="CHRISP" CREATEDON="20110706" CHANGEDBY="CHRISP" CHANGEDON="20110722" PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="INT4" PARVALUE="1"/>
   <parameter CLSNAME="ZGOOGLE_AUTHENTICATE" CMPNAME="CREATE_SECRET" SCONAME="MAX_INCORRECT_LOGINS" VERSION="1" LANGU="E" DESCRIPT="Maximum number of failed logins allowed in 30sec window" CMPTYPE="1" MTDTYPE="0" EDITORDER="4 " DISPID="0 " AUTHOR="CHRISP" CREATEDON="20110706" CHANGEDBY="CHRISP" CHANGEDON="20110722" PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="INT4" PARVALUE="3"/>
   <source>method create_secret.

set_secret_store( secret_store ).

* generate a random bit sequence - needs to be
* SECRET_BITS/8 + SCRATCHCODES*BYTES_PER_SCRATCHCODE long

* using Xstring as known bits per &quot;character&quot;.
  data: l_random_bytes type xstring,
        l_secret type string,
        lt_scratchcodes type stringtab.

  l_random_bytes = generate_random_xstring( ).

* convert this into secret code (rfc3548 http://tools.ietf.org/html/rfc3548 )

  l_secret = base32_encode( l_random_bytes ).

* generate scratch codes

  lt_scratchcodes = generate_scratchcodes( l_random_bytes ).


  mo_secret_store-&gt;set_secret( l_secret ).
  mo_secret_store-&gt;set_scratch_codes( lt_scratchcodes ).

endmethod.</source>
  </method>
  <method CLSNAME="ZGOOGLE_AUTHENTICATE" CMPNAME="GENERATE_RANDOM_XSTRING" VERSION="1" LANGU="E" EXPOSURE="0" STATE="1" EDITORDER="4 " DISPID="0 " AUTHOR="CHRISP" CREATEDON="20110706" CHANGEDBY="CHRISP" CHANGEDON="20110725" MTDTYPE="0" MTDDECLTYP="0" R3RELEASE="701" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZGOOGLE_AUTHENTICATE" CMPNAME="GENERATE_RANDOM_XSTRING" SCONAME="RANDOM_XSTRING" VERSION="1" LANGU="E" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " AUTHOR="CHRISP" CREATEDON="20110706" CHANGEDBY="CHRISP" CHANGEDON="20110706" PARDECLTYP="3" PARPASSTYP="0" TYPTYPE="1" TYPE="XSTRING"/>
   <source>method generate_random_xstring.
  data: lo_random type ref to cl_abap_random,
        l_random_length type i,
        l_seed type i,
        l_random_signed_int type i, &quot;4 bytes 32 bits
        lo_convert_out type ref to cl_abap_conv_out_ce.

* we need enough random bytes to cover the main secret code used to generate
* the time based codes and also enough to give us some scratchcodes.

  l_random_length = ( ( secret_bits / 8 ) + ( scratchcodes * bytes_per_scratchcode ) ) / 4.

*slight worry that abap random is not that random!  As is always generating the same numbers!

* need to seed it

  l_seed = sy-uzeit + sy-datum .

  lo_random = cl_abap_random=&gt;create( l_seed ).
  lo_convert_out = cl_abap_conv_out_ce=&gt;create( ).
  do l_random_length times.
    l_random_signed_int = lo_random-&gt;int( ).
    lo_convert_out-&gt;write( data = l_random_signed_int ).
  enddo.

  random_xstring = lo_convert_out-&gt;get_buffer( ).

endmethod.</source>
  </method>
  <method CLSNAME="ZGOOGLE_AUTHENTICATE" CMPNAME="GENERATE_SCRATCHCODES" VERSION="1" LANGU="E" EXPOSURE="0" STATE="1" EDITORDER="5 " DISPID="0 " AUTHOR="CHRISP" CREATEDON="20110707" CHANGEDBY="CHRISP" CHANGEDON="20110725" MTDTYPE="0" MTDDECLTYP="0" R3RELEASE="701" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZGOOGLE_AUTHENTICATE" CMPNAME="GENERATE_SCRATCHCODES" SCONAME="RANDOM_XSTRING" VERSION="1" LANGU="E" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " AUTHOR="CHRISP" CREATEDON="20110707" CHANGEDON="00000000" PARDECLTYP="0" PARPASSTYP="1" TYPTYPE="1" TYPE="XSTRING"/>
   <parameter CLSNAME="ZGOOGLE_AUTHENTICATE" CMPNAME="GENERATE_SCRATCHCODES" SCONAME="MATCHCODES" VERSION="1" LANGU="E" DESCRIPT="Table with Strings" CMPTYPE="1" MTDTYPE="0" EDITORDER="2 " DISPID="0 " AUTHOR="CHRISP" CREATEDON="20110707" CHANGEDON="00000000" PARDECLTYP="3" PARPASSTYP="0" TYPTYPE="1" TYPE="STRINGTAB"/>
   <source>method generate_scratchcodes.



  data: l_scratchcode type i,
        l_scratchcode_hex type xstring,
        l_hex type xstring,
        l_count type i,
        l_hex_chars_per_scratch type i,
        l_position type i,
        l_max_decimal type i,
        l_min_decimal type i,
        l_max type xstring,
        l_string type string,
        l_code_chars(20) type c.

  l_max = &apos;7FFFFFFF&apos;.


  l_max_decimal = 10 ** scratchcode_length.
  l_min_decimal = l_max_decimal / 10.
  do scratchcodes times.

    l_hex = random_xstring+l_position(bytes_per_scratchcode).
    l_position = l_position + bytes_per_scratchcode.
*convert to unsigned 31 bit
    l_scratchcode_hex = l_hex bit-and l_max.
    l_scratchcode = l_scratchcode_hex.
    l_scratchcode = l_scratchcode mod l_max_decimal.

    if l_scratchcode &lt; l_min_decimal.
      l_scratchcode = l_scratchcode * 10.
* OK to end in zero - just not start with it.
    endif.

    write l_scratchcode to l_code_chars.

    condense l_code_chars no-gaps.

    l_string = l_code_chars.

    append l_string to matchcodes.

  enddo.





endmethod.</source>
  </method>
  <method CLSNAME="ZGOOGLE_AUTHENTICATE" CMPNAME="GET_TIMESTAMP" VERSION="1" LANGU="E" DESCRIPT="get number of windows since 00:00:00 1/1/1970" EXPOSURE="0" STATE="1" EDITORDER="6 " DISPID="0 " AUTHOR="CHRISP" CREATEDON="20110710" CHANGEDBY="CHRISP" CHANGEDON="20110725" MTDTYPE="0" MTDDECLTYP="0" R3RELEASE="701" BCMTDCAT="00" BCMTDSYN="0">
   <parameter CLSNAME="ZGOOGLE_AUTHENTICATE" CMPNAME="GET_TIMESTAMP" SCONAME="TIME_WINDOWS" VERSION="1" LANGU="E" CMPTYPE="1" MTDTYPE="0" EDITORDER="1 " DISPID="0 " AUTHOR="CHRISP" CREATEDON="20110710" CHANGEDON="00000000" PARDECLTYP="3" PARPASSTYP="0" TYPTYPE="1" TYPE="I"/>
   <source>method get_timestamp.

* now ABAP doesn&apos;t support a long in 7.01 but 4 byte int should be good for next 2000 years ;-)

* OK C does this very easily but at least we have some nice date based math in ABAP.

  data: l_days_since type i,
        l_1970 type d,
        l_windows_until_today type i,
        l_windows_today type i.

  l_1970 = &apos;19700101&apos;.

  l_days_since = sy-datum - l_1970.

  l_windows_until_today = l_days_since * 24 * 60 * 2.

  l_windows_today = sy-uzeit / 30.

  time_windows = l_windows_until_today + l_windows_today.

endmethod.</source>
  </method>

  data: lo_nd_verification type ref to if_wd_context_node,
        lo_el_verification type ref to if_wd_context_element,
        ls_verification type wd_this-&gt;element_verification,
        l_is_valid type boole_d,
        ls_message type bapiret2,
        lo_component type ref to if_wd_component,
        lo_fpm type ref to if_fpm.

  if io_event-&gt;mv_event_id = cl_fpm_event=&gt;gc_event_next_step.
* generate a code
    lo_nd_verification = wd_context-&gt;get_child_node( name = wd_this-&gt;wdctx_verification ).
    lo_el_verification = lo_nd_verification-&gt;get_element( ).
    lo_el_verification-&gt;get_static_attributes( importing
      static_attributes = ls_verification ).

    lo_fpm = cl_fpm_factory=&gt;get_instance( ).
    lo_component = wd_this-&gt;wd_get_api( ).
    if ls_verification-verification_code is initial.

      ls_message-type = if_hrpa_message_handler=&gt;error.
      ls_message-id = &apos;ZGOOGLE_AUTHENTICATE&apos;.
      ls_message-number = &apos;001&apos;.
      ls_message-message_v1 = ls_verification-verification_code.
      message e001(zgoogle_authenticate) into ls_message-message
        with ls_verification-verification_code.


      lo_fpm-&gt;mo_message_manager-&gt;report_bapiret2_message(
                      is_bapiret2                  = ls_message
                      io_component                 = lo_component ).


      ev_result = if_fpm_constants=&gt;gc_event_result-failed.
    else.

      l_is_valid =  wd_assist-&gt;verify_code( ls_verification-verification_code ).


      if l_is_valid = abap_false.


        ls_message-type = if_hrpa_message_handler=&gt;error.
        ls_message-id = &apos;ZGOOGLE_AUTHENTICATE&apos;.
        ls_message-number = &apos;000&apos;.
        ls_message-message_v1 = ls_verification-verification_code.
        message e000(zgoogle_authenticate) into ls_message-message
          with ls_verification-verification_code.


        lo_fpm-&gt;mo_message_manager-&gt;report_bapiret2_message(
                        is_bapiret2                  = ls_message
                        io_component                 = lo_component ).


        ev_result = if_fpm_constants=&gt;gc_event_result-failed.

      else.
        ev_result = if_fpm_constants=&gt;gc_event_result-ok.
      endif.
* clear verification code

      lo_el_verification-&gt;set_static_attributes(
         static_attributes = ls_verification ).

    endif.
  endif.
endmethod.
